{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "servicio"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fecha_ingreso": {
      "type": "datetime"
    },
    "fecha_entrega": {
      "type": "datetime"
    },
    "descripcion": {
      "type": "text"
    },
    "diagnostico": {
      "type": "text"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "ingresado, en_diagnostico, en_reparacion, finalizado, entregado"
      ]
    },
    "vehiculo": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vehiculo.vehiculo"
    },
    "mecenico": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mecenico.mecenico",
      "inversedBy": "servicios"
    }
  }
}
