{
  "kind": "collectionType",
  "collectionName": "orden_de_trabajos",
  "info": {
    "singularName": "orden-de-trabajo",
    "pluralName": "orden-de-trabajos",
    "displayName": "orden de trabajo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fecha_ingreso": {
      "type": "date",
      "required": true
    },
    "fecha_estimada": {
      "type": "date"
    },
    "fecha_entrega": {
      "type": "date"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "ingresado",
        "en_diagnostico",
        "en_reparacion",
        "finalizado",
        "facturado",
        "entregado"
      ]
    },
    "descripcion": {
      "type": "text"
    },
    "diagnostico": {
      "type": "text"
    },
    "vehiculo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vehiculo.vehiculo",
      "inversedBy": "orden_de_trabajos"
    },
    "mecanico": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mecenico.mecenico",
      "inversedBy": "orden_de_trabajos"
    },
    "zona": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::zona.zona",
      "inversedBy": "orden_de_trabajos"
    },
    "presupuesto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::presupuesto.presupuesto",
      "inversedBy": "orden_de_trabajo"
    },
    "factura": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::factura.factura",
      "mappedBy": "orden_de_trabajo"
    },
    "repuestos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::repuesto.repuesto",
      "inversedBy": "orden_de_trabajos"
    },
    "bitacoras": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bitacora.bitacora",
      "mappedBy": "orden_de_trabajo"
    },
    "servicios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::servicio.servicio",
      "inversedBy": "orden_de_trabajos"
    },
    "fecha_inicio_planificada": {
      "type": "datetime"
    },
    "fecha_fin_planificada": {
      "type": "datetime"
    },
    "hora_inicio_real": {
      "type": "datetime"
    },
    "hora_fin_real": {
      "type": "datetime"
    },
    "danos_previos": {
      "type": "json"
    },
    "prioridad": {
      "type": "enumeration",
      "enum": [
        "urgente",
        "alta",
        "normal",
        "baja"
      ],
      "default": "normal"
    },
    "kilometraje_ingreso": {
      "type": "integer"
    },
    "nivel_combustible": {
      "type": "enumeration",
      "enum": [
        "vacio",
        "cuarto",
        "medio",
        "tres_cuartos",
        "lleno"
      ]
    }
  }
}