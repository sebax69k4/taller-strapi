{
  "kind": "collectionType",
  "collectionName": "repuestos",
  "info": {
    "singularName": "repuesto",
    "pluralName": "repuestos",
    "displayName": "repuesto"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "descripcion": {
      "type": "text"
    },
    "stock": {
      "type": "integer"
    },
    "precio": {
      "type": "decimal"
    },
    "servicios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::servicio.servicio",
      "inversedBy": "repuestos"
    },
    "stock_minimo": {
      "type": "integer",
      "required": true,
      "default": 5
    },
    "proveedor": {
      "type": "string"
    },
    "ubicacion": {
      "type": "string"
    }
  }
}
