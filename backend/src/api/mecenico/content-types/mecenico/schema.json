{
  "kind": "collectionType",
  "collectionName": "mecenicos",
  "info": {
    "singularName": "mecenico",
    "pluralName": "mecenicos",
    "displayName": "mecanico"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "apellido": {
      "type": "string"
    },
    "especialidad": {
      "type": "string"
    },
    "servicios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::servicio.servicio",
      "mappedBy": "mecenico"
    },
    "orden_de_trabajos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::orden-de-trabajo.orden-de-trabajo",
      "mappedBy": "mecanico"
    },
    "zona_asignada": {
      "type": "string"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "\"disponible\", \"ocupado\", \"descanso\", \"ausente\""
      ]
    },
    "ordenes_activas": {
      "type": "integer"
    },
    "zona": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::zona.zona",
      "inversedBy": "mecanicos"
    },
    "email": {
      "type": "email"
    },
    "zonas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::zona.zona",
      "mappedBy": "mecanico"
    },
    "bitacoras": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bitacora.bitacora",
      "mappedBy": "mecanico"
    },
    "vehiculos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vehiculo.vehiculo",
      "mappedBy": "mecanico"
    }
  }
}
